---
import { Image } from 'astro:assets';
import longFilmImage from '../../assets/gardien-feu-chien-noir.webp';

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Image
  src={longFilmImage}
  alt="Chien noir gardien du feu - Accès aux longs-métrages documentaires de La Jetée Films"
  width={800}
  height={400}
  class={className}
  loading="eager"
  decoding="sync"
  fetchpriority="high"
/> 