---
import { Image } from 'astro:assets';
import teamImage from '../../assets/africa-bayon-scenographe.webp';

interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<Image
  src={teamImage}
  alt="Africa Bayón - Scénographe"
  width={400}
  height={400}
  class={className}
  loading="lazy"
  decoding="async"
/> 